#include <stdio.h>

int main(){
	
	int numero;
	
	printf("Digite um numero\n");
	scanf("%d", &numero);
	
	if(numero < 100){
		
		while(numero < 100){			
			numero = numero + 1;
			printf("%d\n", numero);			
		}
		
	}
	else {		
		printf("O numero digitado deve ser menor que 100!\n");		
	}
	
return(0);	
	
}
